{{ header }}
{{ column_left }}
<div id="content">
	<div class="ms-wrapper">
		<div class="ms-container">
			<div class="ms-page">
				<span class="ms-page__title">Archive</span>
				<div class="ms-page-row">
					<div class="ms-page-col ms-page-col-fluid">
						<div class="ms-page-box">
							<div class="ms-page-box__item">
								<div class="ms-page-box__title">
									<span class="ms-page-box__title-icon">
										<img src="img/icons/icon-files.svg" alt="">
									</span>
									<span class="ms-page-box__title-txt">Last detect</span>
								</div>
								<div class="ms-page-box__content">
									<table>
										<thead>
											<tr>
                      {% if sort == "type_attack" and sort_direction == "ASC" %}
												<th class="text-center">
													<a href="{{type_attack}}" class="ms-arrow-link">Type</a>
												</th>
                      {% elseif sort == "type_attack" and sort_direction == "DESC" %}
                        <th class="text-center">
													<a href="{{type_attack}}" class="ms-arrow-link ms-arrow-link-asc">Type</a>
												</th>
                      {% else %}
                        <th class="text-center">
													<a href="{{type_attack}}">Type</a>
												</th>
                      {% endif %}
                      {% if sort == "ip_attack" and sort_direction == "ASC" %}
												<th class="text-center">
													<a href="{{ip_attack}}" class="ms-arrow-link">IP</a>
												</th>
                      {% elseif sort == "ip_attack" and sort_direction == "DESC" %}
                        <th class="text-center">
													<a href="{{ip_attack}}" class="ms-arrow-link ms-arrow-link-asc">IP</a>
												</th>
                      {% else %}
                        <th class="text-center">
													<a href="{{ip_attack}}">IP</a>
												</th>
                      {% endif %}
                      {% if sort == "time_attack" and sort_direction == "ASC" %}
                        <th class="text-center">
													<a href="{{time_attack}}" class="ms-arrow-link">Time</a>
												</th>									
                      {% elseif sort == "time_attack" and sort_direction == "DESC" %}
                        <th class="text-center">
													<a href="{{time_attack}}" class="ms-arrow-link ms-arrow-link-asc">Time</a>
												</th>
                      {% else %}
                        <th class="text-center">
													<a href="{{time_attack}}">Time</a>
												</th>
                      {% endif %}
											{% if sort == "url_attack" and sort_direction == "ASC" %}
                        <th>
													<a href="{{url_attack}}" class="ms-arrow-link">Path & User agent</a>
												</th>
											{% elseif sort == "url_attack" and sort_direction == "DESC" %}
                        <th>
													<a href="{{url_attack}}" class="ms-arrow-link ms-arrow-link-asc">Path & User agent</a>
												</th>
											{% else %}
                        <th>
													<a href="{{url_attack}}" >Path & User agent</a>
												</th>
                      {% endif %}
												<th class="text-center">Delete</th>
												<th class="text-center">More</th>
											</tr>
										</thead>
										<tbody>
											{% for item in list_attack %}
												<tr>
													<td class="text-center">
														<a href="#" class="ms-shadow-link ms-shadow-link-danger">{{item[4]|escape}}</a>
													</td>
													<td class="text-center">
														<a href="#" class="ms-link-light">{{ item[0]|escape }}</a>
													</td>
													<td class="text-center">{{ item[1]|escape }}</td>
													<td>
														<small class="ms-m-0">{{ item[2]|escape }}</small>
														{{ item[3]|escape }}</td>
													<td class="text-center">
														<a class="ms-btn-icon ms-btn-icon-remove ajax-remove-archive" data-value="{{ item['id'] }}">
															<svg>
																<use xlink:href="#icon-remove"></use>
															</svg>
														</a>
													</td>
													<td class="text-center">
														<button class="ms-btn-icon ms-btn-icon-delete" ></button>
													</td>
												</tr>
												<tr class="ms-hidden">
													<td colspan="6">
														<ul>
															<li>
																<b>Type:</b>
																{{item[4]|escape}}</li>
															<li>
																<b>IP:</b>
																{{item[0]|escape}}</li>
															<li>
																<b>Time:</b>
																{{item[1]|escape}}</li>
															<li>
																<b>Path:</b>
                                {{ item[2]|escape }}</li>
															<li>
																<b>User agent:</b>
																{{item[3]|escape}}</li>
                              {% if item[5] is defined %}
															<li>
																<b>POST:</b>
																{{item[5]|escape}}</li>
                              {% endif %}
														</ul>
													</td>
												</tr>
											{% endfor %}
										</tbody>
									</table>
								</div>
							</div>
						</div>
					{% if num_page > 1 %}
							<ul class="ms-pagination">
							{% if  prev == "disabled" %}
								<li class="ms-pagination__item ms-pagination__item-prev disabled">
									<a  class="ms-pagination__link">
										<svg>
											<use xlink:href="#icon-arrow"></use>
										</svg>
									</a>
								</li>
								{% else %}
								<li class="ms-pagination__item ms-pagination__item-prev">
									<a href="{{ prev }}" class="ajax_pagination ms-pagination__link">
										<svg>
											<use xlink:href="#icon-arrow"></use>
										</svg>
									</a>
								</li>
							{% endif %}
								{% for num, url in urls_pagination %}
									{% if url=="disabled" %}
										<li class="ms-pagination__item current">
											<a class="ms-pagination__link">{{ num }}</a>
										</li>
									{% else %}
										<li class="ms-pagination__item">
											<a href="{{ url }}" class="ajax_pagination ms-pagination__link">{{ num }}</a>
										</li>
									{% endif %}
								{% endfor %}
							{% if next == "disabled" %}
								<li class="ms-pagination__item ms-pagination__item-next disabled">
									<a class="ms-pagination__link">
										<svg>
											<use xlink:href="#icon-arrow"></use>
										</svg>
									</a>
								</li>
							{% else %}
								<li class="ms-pagination__item ms-pagination__item-next">
										<a href="{{ next }}" class="ms-pagination__link ajax_pagination">
											<svg>
												<use xlink:href="#icon-arrow"></use>
											</svg>
										</a>
									</li>
							</ul>
								{% endif %}
						{% endif %}
					</div>
				</div>
			</div>
		</div>
	</div>
</div>


